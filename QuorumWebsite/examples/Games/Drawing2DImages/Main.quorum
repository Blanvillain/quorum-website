use Libraries.Game.Game
use Libraries.Game.Graphics.Drawable
use Libraries.System.File

class Main is Game
    Drawable logo

    action Main
        StartGame()
    end

    action CreateGame
        //path for online
        File file
        file:SetWorkingDirectory("/")
        file:SetPath("media/games/hourofcode.png")
        logo:Load(file)
        Add(logo)
    end

    action Update(number seconds)
    end
end